package eg_if_else_switch;

public class DemoMain {
	public static void main(String[] args) {
		
		int x = 7;
		
		if (x == 8) {
			System.out.println("Yes, it is 8");
		}
		else {
			System.out.println("No, it is not.");
		}
		
		int score = 80;
		if (score >= 80) {
			System.out.println("Top score");
		} else if(score<80 && score >= 60) {
			
		}
	}
}
