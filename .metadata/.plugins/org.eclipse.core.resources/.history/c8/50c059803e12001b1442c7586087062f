package eg3;

public class Animal {
	public static void helloStaticAnimal() {
		System.out.println("Hello from helloStaticAnimal()");
		
	}
	public static void helloStaticAnimal2() {
		System.out.println("Hello from helloStaticAnimal2()");
	}
	
	public void helloNonStatic() {
		System.out.println("Hello from nonStatic cal");
	}

}
